= form_tag import_games_path, multipart: true, :remote => true do
  = label_tag 'Enter competition name'
  = text_field_tag :competition_name
  = label_tag 'Import roster'
  = file_field_tag :file
  = hidden_field_tag :team_name, @auth.current_team
  = link_to('Cancel', '#', :'data-clear-form' => true, id: 'cancel', :class => 'button tiny radius alert')
  = submit_tag 'Import', :class => 'button tiny radius success'

